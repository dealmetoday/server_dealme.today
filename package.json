{
  "name": "dealme.today",
  "version": "1.0.0",
  "description": "dealme.today - online discount platform",
  "main": "./server.js",
  "scripts": {
    "serve": "NODE_ENV=development npm run genkey && nodemon ./server.js || true",
    "serve-prod": "NODE_ENV=production npm run genkey && node server.js -port 5000",
    "clean": "mongo dropDB.js",
    "dbon": "sudo service mongod start",
    "genkey": "./config/keys/rsa_keygen.sh",
    "win-genkey": "./config/keys/rsa_keygen.bat",
    "win-serve": "set NODE_ENV=development npm run win-genkey && nodemon ./server.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/RyanLiu6/server_dealme.today"
  },
  "keywords": [
    "mern",
    "mongodb",
    "mongoose",
    "mongoose-multi",
    "REST",
    "CRUD",
    "API",
    "promotions",
    "deals",
    "discount"
  ],
  "author": "Ryan Liu, Alfred Hong, Mihailo Stefanovic",
  "license": "MIT",
  "dependencies": {
    "argon2": "^0.20.1",
    "cors": "^2.8.4",
    "express": "^4.16.3",
    "express-validator": "^5.3.0",
    "jsonwebtoken": "^8.4.0",
    "mongoose": "^5.2.13",
    "mongoose-multi": "^0.4.4"
  },
  "devDependencies": {
    "nodemon": "^1.18.4"
  }
}
